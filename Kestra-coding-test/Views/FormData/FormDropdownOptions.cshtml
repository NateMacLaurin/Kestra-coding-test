@model IEnumerable<Kestra_coding_test.Models.FormDropdownOptions>

<div class="container">
    <div class="d-flex row pt-4">
        <div class="col-sm-6">
            <h2 class="text-primary">FormDropdownOptions - Data</h2>
        </div>
        <div class="col-sm-6 align-content-end flex-wrap">
            <a class="nav-link text-primary" asp-controller="FormData" asp-action="Index">Back to Form Data</a>
        </div>
    </div>
        
    @if (Model.Count() > 0)
    {
        <table class="table table-bordered table-striped" style="width: 100%">
            <thead>
                <tr>
                    <th>
                        SelectorValue
                    </th>
                    <th>
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var obj in Model)
                {
                    <tr>
                        <td width="50%">@obj.SelectorValue</td>
                    </tr>
                }
            </tbody>
        </table>
    }
    else
    {
        <p class="text-primary">No FormDropdownOptions Exist!</p>
    }
</div>